Cell["\<\
The below cell of Mathematica code contains a module that runs this system of \
equations whose parameters can be easily changed to realize hypothetical \
scenarios (see the call to the module below its definition).\
\>", "Text",
 CellChangeTimes->{{3.859489674527402*^9, 3.859489726647381*^9}, {
  3.8594911250215826`*^9, 
  3.8594911407056575`*^9}},ExpressionUUID->"fb35fd2f-5ba9-4de8-882e-\
eb5c4841e8ff"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Define", " ", "the", " ", "module"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"BaseSIR", "[", 
      RowBox[{
      "St0_", ",", "It0_", ",", "Rt0_", ",", "f_", ",", "r_", ",", "niter_"}],
       "]"}], ":=", 
     RowBox[{"(", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        SubscriptBox["S", "t"], "=", "St0"}], ";", " ", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"Set", " ", "initial", " ", "susceptible"}], ",", " ", 
         "infected", ",", " ", 
         RowBox[{"and", " ", "recovered"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        SubscriptBox["I", "t"], "=", "It0"}], ";", "\[IndentingNewLine]", 
       RowBox[{
        SubscriptBox["R", "t"], "=", "Rt0"}], ";", "\[IndentingNewLine]", 
       RowBox[{"results", " ", "=", " ", 
        RowBox[{"Table", "[", 
         RowBox[{"0", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"niter", " ", "+", " ", "1"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "3"}], "}"}]}], "]"}]}], ";", " ", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{
         "Create", " ", "array", " ", "for", " ", "storing", " ", 
          "iterations"}], ",", " ", 
         RowBox[{"include", " ", "initial", " ", "values"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"results", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"1", ";;", "3"}], "]"}], "]"}], "=", 
        RowBox[{"{", 
         RowBox[{
          SubscriptBox["S", "t"], ",", 
          SubscriptBox["I", "t"], ",", 
          SubscriptBox["R", "t"]}], "}"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "1"}], ",", 
         RowBox[{"i", "<=", "niter"}], ",", 
         RowBox[{"i", "++"}], ",", "\n", "\t", 
         RowBox[{"(*", " ", 
          RowBox[{"Perform", " ", "iteration"}], " ", "*)"}], 
         "\[IndentingNewLine]", "\t", 
         RowBox[{
          RowBox[{
           SubscriptBox["S", 
            RowBox[{"t", "+", "1"}]], "=", " ", 
           RowBox[{
            SubscriptBox["S", "t"], "-", 
            RowBox[{"f", " ", 
             SubscriptBox["S", "t"], 
             SubscriptBox["I", "t"]}]}]}], ";", "\[IndentingNewLine]", "\t", 
          RowBox[{
           SubscriptBox["I", 
            RowBox[{"t", "+", "1"}]], "=", 
           RowBox[{
            SubscriptBox["I", 
             RowBox[{"t", " "}]], "+", 
            RowBox[{"f", " ", 
             SubscriptBox["S", "t"], 
             SubscriptBox["I", "t"]}], " ", "-", " ", 
            RowBox[{
             SubscriptBox["I", "t"], "r"}]}]}], ";", "\[IndentingNewLine]", 
          "\t", 
          RowBox[{
           SubscriptBox["R", 
            RowBox[{"t", "+", "1"}]], "=", 
           RowBox[{
            SubscriptBox["R", "t"], "+", 
            RowBox[{
             SubscriptBox["I", "t"], "r"}]}]}], ";", "\n", "\t", "\n", "\t", 
          RowBox[{"(*", " ", 
           RowBox[{"Update", " ", "values"}], " ", "*)"}], 
          "\[IndentingNewLine]", "\t", 
          RowBox[{
           SubscriptBox["S", "t"], "=", 
           SubscriptBox["S", 
            RowBox[{"t", "+", "1"}]]}], ";", "\[IndentingNewLine]", "\t", 
          RowBox[{
           SubscriptBox["I", "t"], "=", 
           SubscriptBox["I", 
            RowBox[{"t", "+", "1"}]]}], ";", "\[IndentingNewLine]", "\t", 
          RowBox[{
           SubscriptBox["R", "t"], "=", 
           SubscriptBox["R", 
            RowBox[{"t", "+", "1"}]]}], ";", "\[IndentingNewLine]", "\t", 
          RowBox[{
           RowBox[{
            RowBox[{"results", "[", 
             RowBox[{"[", 
              RowBox[{"i", "+", "1"}], "]"}], "]"}], "[", 
            RowBox[{"[", 
             RowBox[{"1", ";;", "3"}], "]"}], "]"}], "=", 
           RowBox[{"{", 
            RowBox[{
             SubscriptBox["S", 
              RowBox[{"t", "+", "1"}]], ",", 
             SubscriptBox["I", 
              RowBox[{"t", "+", "1"}]], ",", 
             SubscriptBox["R", 
              RowBox[{"t", "+", "1"}]]}], "}"}]}], ";"}]}], " ", 
        RowBox[{"(*", " ", 
         RowBox[{"Store", " ", "values"}], " ", "*)"}], "\[IndentingNewLine]",
         "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"ListPlot", "[", 
        RowBox[{
         RowBox[{"Transpose", "[", "results", "]"}], ",", 
         RowBox[{"Joined", "->", "True"}], ",", " ", 
         RowBox[{"PlotLegends", "\[Rule]", 
          RowBox[{"{", 
           RowBox[{"\"\<S\>\"", ",", "\"\<I\>\"", ",", "\"\<R\>\""}], 
           "}"}]}]}], "]"}]}], " ", 
      RowBox[{"(*", " ", 
       RowBox[{"Return", " ", "plot", " ", "of", " ", "array"}], " ", "*)"}], 
      "\[IndentingNewLine]", ")"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"example1", " ", "=", " ", 
    RowBox[{"BaseSIR", "[", 
     RowBox[{
     "300", ",", "100", ",", "0", ",", "0.0015", ",", "0.4", ",", "30"}], 
     "]"}], " ", 
    RowBox[{"(*", " ", 
     RowBox[{"Call", " ", "module"}], " ", "*)"}]}]}]}]], "Code",
 InitializationCell->True,
