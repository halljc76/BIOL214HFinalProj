Cell["\<\
Consider the following code for this model. Note how, in this example, one \
virus is clearly more infectious as the other, though both exhibit similar \
behavior throughout the simulation.\
\>", "Text",
 CellChangeTimes->{{3.8595060847407007`*^9, 3.859506138127557*^9}, {
  3.8595061704647975`*^9, 
  3.859506210043322*^9}},ExpressionUUID->"6c40b5c2-2ff5-412c-965b-\
dad3071ab896"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"MultiStrainSIRV", "[", 
   RowBox[{
   "St0_", ",", "I1t0_", ",", "I2t0_", ",", "Rt0_", ",", "Vt0_", ",", "f1_", 
    ",", "f2_", ",", " ", "r1_", ",", " ", "r2_", ",", "b_", ",", "niter_"}], 
   "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     SubscriptBox["S", "t"], "=", "St0"}], ";", " ", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{"Set", " ", "initial", " ", "susceptible"}], ",", " ", 
      "infected", ",", " ", 
      RowBox[{"and", " ", "recovered"}]}], " ", "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["I", 
      RowBox[{"1", ",", "t"}]], "=", "I1t0"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["I", 
      RowBox[{"2", ",", "t"}]], "=", "I2t0"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["R", "t"], "=", "Rt0"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     SubscriptBox["V", "t"], "=", "Vt0"}], ";", "\[IndentingNewLine]", 
    RowBox[{"results", " ", "=", " ", 
     RowBox[{"Table", "[", 
      RowBox[{"0", ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", 
         RowBox[{"niter", " ", "+", " ", "1"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"j", ",", "5"}], "}"}]}], "]"}]}], ";", " ", 
    RowBox[{"(*", " ", 
     RowBox[{
      RowBox[{
      "Create", " ", "array", " ", "for", " ", "storing", " ", "iterations"}],
       ",", " ", 
      RowBox[{"include", " ", "initial", " ", "values"}]}], " ", "*)"}], 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"results", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "[", 
      RowBox[{"[", 
       RowBox[{"1", ";;", "5"}], "]"}], "]"}], "=", 
     RowBox[{"{", 
      RowBox[{
       SubscriptBox["S", "t"], ",", 
       SubscriptBox["I", 
        RowBox[{"1", ",", "t"}]], ",", 
       SubscriptBox["I", 
        RowBox[{"2", ",", "t"}]], ",", 
       SubscriptBox["R", "t"], ",", 
       SubscriptBox["V", "t"]}], "}"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"i", "=", "1"}], ",", 
      RowBox[{"i", "<=", "niter"}], ",", 
      RowBox[{"i", "++"}], ",", "\n", "\t", 
      RowBox[{"(*", " ", 
       RowBox[{"Perform", " ", "iteration"}], " ", "*)"}], 
      "\[IndentingNewLine]", "\t", 
      RowBox[{
       RowBox[{
        SubscriptBox["S", 
         RowBox[{"t", "+", "1"}]], "=", " ", 
        RowBox[{
         SubscriptBox["S", "t"], "-", 
         RowBox[{
          SubscriptBox["S", "t"], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"f1", " ", 
             SubscriptBox["I", 
              RowBox[{"1", ",", "t"}]]}], "+", 
            RowBox[{"f2", " ", 
             SubscriptBox["I", 
              RowBox[{"2", ",", "t"}]]}]}], ")"}]}]}]}], ";", 
       "\[IndentingNewLine]", "\t", 
       RowBox[{
        SubscriptBox["I", 
         RowBox[{"1", ",", 
          RowBox[{"t", "+", "1"}]}]], "=", 
        RowBox[{
         SubscriptBox["I", 
          RowBox[{"1", ",", "t", " "}]], "+", 
         RowBox[{"f1", " ", 
          SubscriptBox["S", "t"], 
          SubscriptBox["I", 
           RowBox[{"1", ",", "t"}]]}], " ", "-", " ", 
         RowBox[{
          SubscriptBox["I", 
           RowBox[{"1", ",", "t"}]], "r1"}]}]}], ";", "\[IndentingNewLine]", 
       "\t", 
       RowBox[{
        SubscriptBox["I", 
         RowBox[{"2", ",", 
          RowBox[{"t", "+", "1"}]}]], "=", 
        RowBox[{
         SubscriptBox["I", 
          RowBox[{"2", ",", "t", " "}]], "+", 
         RowBox[{"f2", "  ", 
          SubscriptBox["S", "t"], 
          SubscriptBox["I", 
           RowBox[{"2", ",", "t"}]]}], " ", "-", " ", 
         RowBox[{
          SubscriptBox["I", 
           RowBox[{"2", ",", "t"}]], "r2"}]}]}], ";", "\[IndentingNewLine]", 
       "\t", 
       RowBox[{
        SubscriptBox["R", 
         RowBox[{"t", "+", "1"}]], "=", 
        RowBox[{
         SubscriptBox["R", "t"], "+", 
         RowBox[{
          SubscriptBox["I", 
           RowBox[{"1", ",", "t"}]], "r1"}], "+", 
         RowBox[{
          SubscriptBox["I", 
           RowBox[{"2", ",", "t"}]], "r2"}]}]}], ";", "\[IndentingNewLine]", 
       "\t", 
       RowBox[{
        SubscriptBox["V", 
         RowBox[{"t", "+", "1"}]], "=", " ", 
        RowBox[{
