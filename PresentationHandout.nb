Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"CustomVaccineFunction", "[", 
     RowBox[{"type_", ",", "t_", ",", "decayTerm_", ",", "const_"}], "]"}], ":=", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"type", "==", "1"}], ",", "\[IndentingNewLine]", "\t", 
       RowBox[{"Return", "[", 
        RowBox[{"1", " ", "-", " ", 
         RowBox[{"(", 
          RowBox[{"1", " ", "/", " ", 
           RowBox[{"(", 
            RowBox[{"1", " ", "+", " ", 
             RowBox[{"Exp", "[", 
              RowBox[{"-", 
               RowBox[{"(", 
                RowBox[{"t", "-", "decayTerm"}], ")"}]}], "]"}]}], ")"}]}], 
          ")"}]}], "]"}], ",", " ", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"Transformation", " ", "of", " ", "the", " ", "Sigmoid", " ", 
          RowBox[{"Function", ":", " ", "Slow"}]}], ",", " ", 
         RowBox[{"logistic", " ", "decay"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", "\t", 
